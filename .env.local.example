# Local environment variables
# DO NOT commit this file to version control
# This file overrides values in .env with real API keys and secrets
# Make a copy of this file named .env.local and add your real keys

# NextAuth
NEXTAUTH_SECRET="your_real_nextauth_secret"

# JWT
JWT_SECRET="your_real_jwt_secret"
JWT_REFRESH_SECRET="your_real_jwt_refresh_secret"

# Google Maps
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your_real_google_maps_api_key"

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY="your_real_stripe_publishable_key"
STRIPE_SECRET_KEY="your_real_stripe_secret_key"
STRIPE_WEBHOOK_SECRET="your_real_stripe_webhook_secret"

# Stripe Price IDs (if different from defaults)
STRIPE_WEEKLY_PRICE_ID="your_real_stripe_weekly_price_id"
STRIPE_BIWEEKLY_PRICE_ID="your_real_stripe_biweekly_price_id"
STRIPE_MONTHLY_PRICE_ID="your_real_stripe_monthly_price_id"

# Resend (Email)
RESEND_API_KEY="your_real_resend_api_key"

# AWS
AWS_ACCESS_KEY_ID="your_real_aws_access_key_id"
AWS_SECRET_ACCESS_KEY="your_real_aws_secret_access_key"
AWS_REGION="your_aws_region"
AWS_BUCKET_NAME="your_aws_bucket_name"

# Cron Jobs
CRON_SECRET="your_real_cron_secret"
CRON_API_KEY="your_real_cron_api_key"

# Admin
ADMIN_EMAIL="your_real_admin_email"

# Cash App
CASHAPP_CLIENT_ID="your_real_cashapp_client_id"
CASHAPP_CLIENT_SECRET="your_real_cashapp_client_secret"

# GitHub Token (for deployment only)
GITHUB_TOKEN="your_real_github_token" 