    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/leaflet": "^1.9.17",
    "@types/pdfkit": "^0.13.9",
    "@types/uuid": "^10.0.0",
    "@vercel/kv": "^3.0.0",
