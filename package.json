{
  "name": "scoopify-club",
  "version": "0.1.1",
  "description": "A comprehensive system for managing dog waste removal services, employee scheduling, and customer subscriptions.",
  "private": true,
  "engines": {
    "node": ">=20.x"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "node scripts/start-dev.js",
    "dev:simple": "node scripts/start-simple.js",
    "dev:secure": "node scripts/secure-dev.js",
    "dev:all": "node scripts/start-dev.js",
    "dev:https": "node scripts/generate-cert.js && next dev",
    "restart": "node restart-dev.js",
    "proxy": "local-ssl-proxy --source 3001 --target 3000",
    "build": "next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "setup-stripe": "ts-node scripts/setup-stripe.ts",
    "test": "jest",
    "test:jest": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:setup": "node scripts/setup-playwright.js",
    "test:ci": "jest --ci && playwright test --ci",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:app": "ts-node scripts/test-app.ts",
    "test:setup": "ts-node scripts/setup-test-db.ts",
    "test:db:create": "ts-node scripts/create-test-db.ts",
    "test:db:setup": "ts-node scripts/setup-test-db.ts",
    "test:db:sqlite": "node scripts/setup-sqlite-test-db.js",
    "test:all": "node scripts/run-tests.js",
    "test:with-server": "node scripts/run-tests.js --with-server",
    "schedule-jobs": "ts-node scripts/schedule-jobs.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "deploy": "node scripts/deploy-to-vercel.js",
    "deploy:prod": "node scripts/deploy-to-vercel.js --production",
    "prepare:vercel": "node scripts/prepare-postgresql-migration.js",
    "setup:vercel": "node scripts/setup-vercel-env.js",
    "vercel:production": "npm run prepare:vercel && npm run setup:vercel && npm run deploy:prod",
    "test:stripe": "node scripts/test-stripe-integration.js",
    "test:payment-retry": "node scripts/test-payment-retry.js",
    "cleanup:stripe": "node scripts/cleanup-stripe-tests.js",
    "cleanup:retry": "node scripts/cleanup-retry-tests.js",
    "retry-payments": "node scripts/retry-failed-payments.js",
    "playwright:install": "playwright install --with-deps"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@braintree/sanitize-url": "^7.1.1",
