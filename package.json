    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.5",
    "glob": "^11.0.1",
    "jose": "^5.10.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lru-cache": "^10.2.0",
    "lucide-react": "^0.503.0",
    "next": "^15.3.1",
    "next-auth": "^4.24.5",
    "next-themes": "^0.4.6",
    "pdfkit": "^0.14.0",
    "react": "^18.2.0",
    "react-day-picker": "^9.6.7",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-leaflet": "^4.2.1",
    "react-map-gl": "^7.1.7",
    "react-query": "^3.39.3",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.4",
    "recharts": "^2.15.3",
    "resend": "^4.3.0",
    "sonner": "^2.0.3",
    "stripe": "^14.17.0",
    "swagger-ui-react": "^5.21.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "prisma": "^5.10.2",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
